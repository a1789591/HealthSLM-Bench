# Environment Variables for HealthSLM-Bench
# Copy this file to .env and fill in your actual values

# Hugging Face token for accessing gated models (e.g., Gemma)
# Get your token from: https://huggingface.co/settings/tokens
HF_TOKEN=your_huggingface_token_here

# Your Hugging Face username for model uploads
# This will be used when pushing fine-tuned models to the hub
HUB_USERNAME=your_username

# Local path to your models directory (when using --pc 1)
# This should point to where you store your local model files
MODELS_PATH=./models

# Example values (replace with your actual values):
# HF_TOKEN=hf_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
# HUB_USERNAME=your_hf_username
# MODELS_PATH=/path/to/your/models
